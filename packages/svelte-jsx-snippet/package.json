{
  "name": "svelte-jsx-snippet",
  "version": "0.5.0",
  "description": "Write JSX as Svelte snippets for testing.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "default": "./dist/index-server.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "default": "./dist/vite.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime/index.d.ts",
      "default": "./dist/jsx-runtime/index.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-runtime/index.d.ts",
      "default": "./dist/jsx-runtime/index.js"
    },
    "./unplugin": {
      "types": "./dist/unplugin/index.d.ts",
      "default": "./dist/unplugin/index.js"
    }
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "clean": "node -e \"fs.rmSync('dist', { recursive: true, force: true })\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssssota/svelte-jsx-snippet.git"
  },
  "keywords": [
    "svelte",
    "jsx",
    "snippet"
  ],
  "files": [
    "dist"
  ],
  "author": "TOMIKAWA Sotaro <tommysouta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ssssota/svelte-jsx-snippet/issues"
  },
  "homepage": "https://github.com/ssssota/svelte-jsx-snippet#readme",
  "devDependencies": {
    "@babel/types": "^7.24.8",
    "svelte": "5.0.0-next.183",
    "svelte-jsx-snippet": "workspace:*",
    "typescript": "^5.5.3",
    "vite": "^5.0.3",
    "vite-plugin-doctest": "^1.0.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "svelte": ">=5.0.0-next.183"
  },
  "dependencies": {
    "@babel/parser": "^7.24.8",
    "magic-string": "^0.30.10",
    "unplugin": "^1.11.0"
  }
}
