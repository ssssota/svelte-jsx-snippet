{
  "name": "svelte-jsx-snippet",
  "version": "0.6.0",
  "description": "Write JSX as Svelte snippets for testing.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "default": "./dist/vite.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime/index.d.ts",
      "default": "./dist/jsx-runtime/index.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-runtime/index.d.ts",
      "default": "./dist/jsx-runtime/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssssota/svelte-jsx-snippet.git"
  },
  "keywords": [
    "svelte",
    "jsx",
    "snippet"
  ],
  "files": [
    "dist"
  ],
  "author": "TOMIKAWA Sotaro <tommysouta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ssssota/svelte-jsx-snippet/issues"
  },
  "homepage": "https://github.com/ssssota/svelte-jsx-snippet#readme",
  "devDependencies": {
    "@babel/generator": "^7.24.10",
    "@babel/parser": "^7.24.8",
    "@babel/types": "^7.24.9",
    "@rollup/pluginutils": "^5.1.0",
    "@types/babel__generator": "^7.6.8",
    "magic-string": "^0.30.10",
    "svelte": "5.0.0-next.183",
    "svelte-jsx-snippet": "workspace:*",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vitest": "^2.0.3"
  },
  "peerDependencies": {
    "svelte": ">=5.0.0-next.183",
    "vite": ">=5.0.0"
  }
}
